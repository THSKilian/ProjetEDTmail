<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetedtmail">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetEDTmail">

        <activity
            android:name=".ScanQrCodeActivity"
            android:label="Scan QR code" />

        <activity
            android:name=".FilterEventActivity"
            android:label="Liste des événements masqués"/>

        <activity
            android:name=".SettingsActivity"
            android:label="@string/activity_settings_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>